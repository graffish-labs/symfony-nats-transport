{
    "name": "graffish-labs/symfony-nats-transport",
    "description": "NATS transport for Symfony applications",
    "type": "library",
    "license": "MIT",
    "version": "1.0.3",
    "autoload": {
        "psr-4": {
            "GraffishLabs\\SymfonyNatsTransport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraffishLabs\\SymfonyNatsTransport\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0|^7.0|^8.0",
        "symfony/messenger": "^7.2|^8.0",
        "basis-company/nats": "^1.0",
        "symfony/uid": "^7.2|^8.0",
        "symfony/serializer": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.0|^7.0"
    },
    "minimum-stability": "stable"
}
